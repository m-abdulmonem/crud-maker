{
  "name": "mabdulmonem/crud-maker",
  "description": "A Laravel package for generating CRUD operations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed Abdulmonem",
      "email": "mabdulalmonem@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mabdulmonem\\CrudMaker\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": ">=8.0"
  },
  "config": {
